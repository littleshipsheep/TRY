#include "mbed.h"

//DigitalOut myled(LED1);

int main() {
    RCC->AHB1ENR|=(1<<1);
    GPIOB->MODER&=(0x03<<(2*0));
    GPIOB->MODER|=(0x01<<(2*0));
    //GPIOB->ODR &= (1<<0);
    

    while(1) {
        GPIOB->ODR |=(1<<0);
//        myled = 1; // LED is ON
        wait(0.2); // 200 ms
        GPIOB->ODR &=~(1<<0);
//        myled = 0; // LED is OFF
        wait(1.0); // 1 sec
    }
}
